	<div class="row">
		<div class="large-6 medium-6 small-12 columns">
			<div class="row">
				<div class="large-6 medium-6 small-12 columns">
					@Html.LabelTextBoxFor(m => m.AppNameFirst, null, new { placeholder = "required" })
