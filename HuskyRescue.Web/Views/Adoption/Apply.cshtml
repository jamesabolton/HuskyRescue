@using HuskyRescue.Web.HtmlHelpers;
@model HuskyRescue.ViewModel.Adoption.Apply

@section scripts {
	@Scripts.Render("~/bundles/Adoption-Apply")
}

<div class="row">
	<div class="small-9 small-centered columns">
		<h2>Adoption Application</h2>
	</div>
</div>

@using (Html.BeginForm("Apply", "Adoption", FormMethod.Post, new { id = "applyForm" }))
