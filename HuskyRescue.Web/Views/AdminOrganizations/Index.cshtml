@model IEnumerable<HuskyRescue.ViewModel.Organization.List>

<div class="row">
	<div class="large-12 columns">
		<h4>Organizations</h4>
	</div>
</div>

<div class="row">
	<div class="large-12 columns">
		<p>
			@Html.ActionLink("Create New", "Create")
		</p>
	</div>
</div>

<div class="row">
	<div class="large-12 columns">
		<table class="table">
			<tr>
				<th>
					@Html.DisplayNameFor(model => model.IsActive)
				</th>
				<th>
					@Html.DisplayNameFor(model => model.IsDeleted)
				</th>
				<th>
					@Html.DisplayNameFor(model => model.IsBoardingPlace)
				</th>
				<th>
					@Html.DisplayNameFor(model => model.IsAnimalClinic)
				</th>
				<th>
					@Html.DisplayNameFor(model => model.IsDonor)
				</th>
				<th>
					@Html.DisplayNameFor(model => model.Name)
				</th>
				<th>
					@Html.DisplayNameFor(model => model.PhoneNumber1)
				</th>
				<th>
					@Html.DisplayNameFor(model => model.Email1)
				</th>
				<th></th>
			</tr>

			@foreach (var item in Model)
			{
				<tr>
					<td>
						@Html.DisplayFor(modelItem => item.IsActive)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.IsDeleted)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.IsBoardingPlace)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.IsAnimalClinic)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.IsDonor)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.Name)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.PhoneNumber1)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.Email1)
					</td>
					<td>
						@Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
						@*@Html.ActionLink("Details", "Details", new { id=item.Id }) |
							@Html.ActionLink("Delete", "Delete", new { id=item.Id })*@
					</td>
				</tr>
			}

		</table>
	</div>
</div>
