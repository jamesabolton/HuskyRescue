		@RenderBody()

			<div id="newsletter">
				<div class="row">
					<div class="medium-8 columns">
						<h5>Stay on top of what&rsquo;s happening at Texas Husky Rescue.</h5>
						<p>Sign up to receive regular digital newsletters and announcements.</p>
					</div>
					<div class="medium-4 columns">
						@using (Html.BeginForm("EmailSignUp", "Home", FormMethod.Post, new { id = "emailSignupForm" }))
						{
							@Html.AntiForgeryToken()
							<div class="row collapse">
								@*margintop-20px*@
								<div class="small-8 medium-8 columns">
									<input type="email" name="email" placeholder="signup@example.com">
								</div>
								<div class="small-4 medium-4 columns">
									<input type="submit" href="#" class="postfix small button expand" value="Sign Up">
								</div>
							</div>
						}
					</div>
				</div>
			</div>
