@model HuskyRescue.ViewModel.Admin.ResourceManager.ResourceDetail

<div class="row">
	<div class="large-12 columns">
		
		<div class="row">
			<div class="large-6 small-12 small-centered large-centered columns"><h2>Resource Details</h2></div>
		</div>

		<div class="row">
			<div class="large-2 columns">@Html.LabelFor(m => m.Name)</div>
			<div class="large-10 columns">@Html.DisplayFor(m => m.Name)</div>
		</div>
		<div class="row">
			<div class="large-2 columns">@Html.LabelFor(m => m.Operations)</div>
			<div class="large-10 columns">@Html.DisplayFor(m => m.Operations)</div>
		</div>
		<div class="row">
			<div class="large-2 columns">@Html.LabelFor(m => m.AssociatedRoleNames)</div>
			<div class="large-10 columns">@Html.DisplayFor(m => m.AssociatedRoleNames)</div>
		</div>
	</div>
</div>